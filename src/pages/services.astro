---
import BaseLayout from "../layouts/BaseLayout.astro";
import ColumnLayout from "../layouts/ColumnLayout.astro";
import { getEntry } from "astro:content";

const services = await getEntry("markdown", "services");
const { Content } = await services.render();
---

<BaseLayout pageTitle={services.data.title}>
  <ColumnLayout pageData={services}>
    <Content />
  </ColumnLayout>
</BaseLayout>
