---
import BaseLayout from "../layouts/BaseLayout.astro";
import ColumnLayout from "../layouts/ColumnLayout.astro";
import { getEntry } from "astro:content";

const brands = await getEntry("markdown", "brands");
const { Content } = await brands.render();
---

<BaseLayout pageTitle={brands.data.title}>
  <ColumnLayout pageData={brands}>
    <Content />
  </ColumnLayout>
</BaseLayout>
