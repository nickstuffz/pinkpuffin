---
import BaseLayout from "../layouts/BaseLayout.astro";
import ColumnLayout from "../layouts/ColumnLayout.astro";
import { getEntry } from "astro:content";

const demo = await getEntry("markdown", "demo");
const { Content } = await demo.render();
---

<BaseLayout pageTitle={demo.data.title}>
  <ColumnLayout pageData={demo}>
    <Content />
  </ColumnLayout>
</BaseLayout>
