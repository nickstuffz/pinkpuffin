---
import BaseLayout from "../layouts/BaseLayout.astro";
import ColumnLayout from "../layouts/ColumnLayout.astro";
import { getEntry } from "astro:content";

const kidsbikes = await getEntry("markdown", "kidsbikes");
const { Content } = await kidsbikes.render();
---

<BaseLayout pageTitle={kidsbikes.data.title}>
  <ColumnLayout pageData={kidsbikes}>
    <Content />
  </ColumnLayout>
</BaseLayout>
