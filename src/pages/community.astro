---
import BaseLayout from "../layouts/BaseLayout.astro";
import ColumnLayout from "../layouts/ColumnLayout.astro";
import { getEntry } from "astro:content";

const community = await getEntry("markdown", "community");
const { Content } = await community.render();
---

<BaseLayout pageTitle={community.data.title}>
  <ColumnLayout pageData={community}>
    <Content />
  </ColumnLayout>
</BaseLayout>
