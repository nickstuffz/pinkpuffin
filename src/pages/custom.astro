---
import BaseLayout from "../layouts/BaseLayout.astro";
import ColumnLayout from "../layouts/ColumnLayout.astro";
import { getEntry } from "astro:content";

const custom = await getEntry("markdown", "custom");
const { Content } = await custom.render();
---

<BaseLayout pageTitle={custom.data.title}>
  <ColumnLayout pageData={custom}>
    <Content />
  </ColumnLayout>
</BaseLayout>
