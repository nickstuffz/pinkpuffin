---
import BaseLayout from "../layouts/BaseLayout.astro";
import ColumnLayout from "../layouts/ColumnLayout.astro";
import { Content, frontmatter } from "../content/markdown/custom.md";
import image1 from "../assets/bikeMeasure.webp";

const images = [{ image: image1, alt: "bicycle with measurements" }];
---

<BaseLayout pageTitle={frontmatter.title}>
  <ColumnLayout images={images}>
    <Content />
  </ColumnLayout>
</BaseLayout>
